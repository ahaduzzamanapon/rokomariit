RewriteEngine On

RewriteCond %{REQUEST_URI} ^/system.*
RewriteRule ^(.*)$ index.php?/$1 [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.+)$ index.php?/$1 [L]


#Force www:
RewriteCond %{HTTP_HOST} ^ausbd.com.au [NC]
RewriteRule ^(.*)$ http://www.ausbd.com.au/$1 [L,R=301,NC]